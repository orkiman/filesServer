{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "PKG_CONFIG_PATH": "${env:PKG_CONFIG_PATH}:/usr/local/lib/pkgconfig",
                "CGO_CPPFLAGS": "-I/usr/local/include/opencv4",
                "CGO_LDFLAGS": "-L/usr/local/lib -lopencv_core -lopencv_dnn -lopencv_imgproc"
            }
        }
    ]
}